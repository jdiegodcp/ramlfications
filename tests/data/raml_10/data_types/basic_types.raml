#%RAML 1.0
title: My API With Types
types:
  Person:
    type: object
    properties:
      name:
        required: true
        type: string
  Place:
    type: object
    properties:
      city:
        required: true
        type: string
      state:
        required: true
        type: string
      province:
        required: false
        type: string
  Thing:
    type: string
