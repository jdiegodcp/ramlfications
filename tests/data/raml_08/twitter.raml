  /destroy{mediaTypeExtension}:
      #TODO Either a screen_name or a user_id must be provided.
      # See https://dev.twitter.com/docs/api/1.1/post/friendships/destroy
    type: base
    post:
      description: |
        Allows the authenticating user to unfollow the user specified in the ID
        parameter.
        Returns the unfollowed user in the requested format when successful. Returns
        a string describing the failure condition when unsuccessful.
        Actions taken in this method are asynchronous and changes will be eventually
        consistent.
      body:
        application/x-www-form-urlencoded:
          formParameters:
            user_id:
              description: |
                The ID of the user who should receive the direct message. Helpful for
                disambiguating when a valid user ID is also a valid screen name.
              type: integer
            screen_name:
              description: |
                The screen name of the user who should receive the direct message.
                Helpful for disambiguating when a valid screen name is also a user ID.
              type: string
