                      },
                      "show_all_inline_media": {
                          "type": "boolean"
                      },
                      "sleep_time": {
                          "properties": {
                              "enabled": {
                                  "type": "boolean"
                              },
                              "end_time": {
                                  "type": "string"
                              },
                              "start_time": {
                                  "type": "string"
                              }
                          },
                          "type": "object"
                      },
                      "time_zone": {
                          "properties": {
                              "name": {
                                  "type": "string"
                              },
                              "tzinfo_name": {
                                  "type": "string"
                              },
                              "utc_offset": {
                                  "type": "integer"
